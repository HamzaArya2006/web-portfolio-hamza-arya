VITE_FORM_ENDPOINT=/api/contact
# FORMS_WEBHOOK_URL can be a Slack/Discord/Make/Zapier webhook to receive submissions
FORMS_WEBHOOK_URL=
# Minimum time in ms a user should spend before submitting (anti-bot)
FORMS_MIN_SUBMIT_MS=3000

# Analytics (optional)
VITE_PLAUSIBLE_DOMAIN=

# Admin API configuration
VITE_PUBLIC_API_URL=http://localhost:3001

# Server environment variables (see server/.env.example)
# JWT_SECRET=your-secret-here (required in production)
# JWT_EXPIRES_IN=24h
# PORT=3001
# CORS_ORIGIN=http://localhost:3000

